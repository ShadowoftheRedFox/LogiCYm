<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" type="AnchorPane" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ScrollPane prefWidth="200.0">
               <content>
                  <VBox fx:id="sidebarVBox" prefWidth="200.0" spacing="10">
                     <children>
                        <ToolBar prefHeight="40.0" prefWidth="200.0">
                          <items>
                            <Button alignment="CENTER" mnemonicParsing="false" onAction="#click" text="Return" textAlignment="CENTER" />
                          </items>
                        </ToolBar>
                        <ImageView fx:id="andGate" fitHeight="108.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/AND.png" />
                           </image>
                        </ImageView>
                        <ImageView fx:id="orGate" fitHeight="108.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/OR.png" />
                           </image>
                        </ImageView>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../../CV/IMG_3268.jpg" />
                           </image>
                        </ImageView>
                        <!-- Add more ImageViews as needed -->
                     </children>
                  </VBox>
               </content>
            </ScrollPane>

            <Pane fx:id="workbenchPane" style="-fx-background-color: lightgray;" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
</fx:root>
